{
  "name": "pandora-box",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "client:build": "vite build",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "server": "bun run --watch server/index.ts",
    "server:build": "bun build server/index.ts --target bun --outdir ./build",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@eslint/js": "9.37.0",
    "@sveltejs/vite-plugin-svelte": "6.2.1",
    "@tsconfig/svelte": "5.0.5",
    "@vitest/browser": "3.2.4",
    "bun-types": "1.2.23",
    "drizzle-kit": "0.31.5",
    "drizzle-orm": "0.44.6",
    "eslint": "9.37.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-svelte": "3.12.4",
    "globals": "16.4.0",
    "playwright": "1.55.1",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "svelte": "5.39.8",
    "svelte-check": "4.3.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.45.0",
    "vite": "7.1.9",
    "vitest": "3.2.4",
    "vitest-browser-svelte": "1.1.0"
  },
  "dependencies": {
    "@elysiajs/cors": "1.4.0",
    "@elysiajs/openapi": "1.4.11",
    "@elysiajs/static": "1.4.0",
    "@unocss/reset": "66.5.2",
    "elysia": "1.4.9",
    "nanostores": "1.0.1",
    "svelte5-router": "3.0.2",
    "unocss": "66.5.2",
    "valibot": "1.1.0",
    "wretch": "2.11.0"
  }
}
